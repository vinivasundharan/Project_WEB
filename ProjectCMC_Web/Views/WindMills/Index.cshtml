            @Html.DisplayNameFor(model => model.WindPark.WindParkName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WindMillName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedOn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModifiedOn)
